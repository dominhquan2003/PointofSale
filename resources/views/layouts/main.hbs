    <script>
        document.addEventListener('DOMContentLoaded', function () {
            function hideElementWithTimeout(element, timeout) {
                if (element) {
                    setTimeout(function () {
                        element.style.transition = "opacity 0.5s";
                        element.style.opacity = 0;

                        setTimeout(function () {
                            element.style.display = "none";
                        }, 500);
                    }, timeout);
                }
            }
            hideElementWithTimeout(document.querySelector('.alert-success'), 2000);
            hideElementWithTimeout(document.querySelector('.alert-primary'), 4000);
            hideElementWithTimeout(document.querySelector('.alert-danger'), 2000);
           
            

            
        });

    </script>

